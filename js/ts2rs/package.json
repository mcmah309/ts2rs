{
  "name": "ts2rs",
  "version": "0.1.7",
  "license": "Apache-2.0",
  "author": {
    "name": "mcmah309"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcmah309/ts2rs.git"
  },
  "keywords": [
    "typescript",
    "rust",
    "serde",
    "codegen"
  ],
  "type": "module",
  "description": "Generate Rust type from TypeScript types, enabling seamless data interchange between TypeScript and Rust applications via JSON. Nested types are traversed, even types in different packages.",
  "bin": {
    "ts2rs": "./dist/bundle/cli.bundle.js"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./bundle/*": "./dist/bundle/*.js"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run ../build.ts",
    "build:release": "NODE_ENV=production bun run ../build.ts",
    "watch": "bun run ../build.ts --watch",
    "test": "bun test",
    "generate": "bun run ./src/cli.bundle.ts",
    "prepublishOnly": "bun run build:release"
  },
  "dependencies": {
    "commander": "catalog:",
    "ts-morph": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:"
  }
}